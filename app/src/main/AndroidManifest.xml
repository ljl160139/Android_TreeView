<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kl.tree.treeview">

    <application
        android:name=".view.CustomerApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.kl.tree.treeview.view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".view.ExpdLvActivity"/>

        <activity android:name=".view.LvTreeActivity"/>

        <activity android:name=".view.RecvTreeActivity"/>

        <provider
            android:authorities="com.kl.tree.test.provider"
            android:name="com.kl.tree.treeview.data.provider.TestProvider"
            android:exported="false"/>
    </application>

</manifest>